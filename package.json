{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --ignore src/assets/ --ignore src/public --ignore dist/ index.js",
    "start": "cross-env-shell NODE_ENV=production \"npm run build && node index.js\"",
    "build": "cross-env NODE_ENV=production npm-run-all --parallel clean lint && webpack --colors --display-error-details --config build/webpack.config.js",
    "lint": "esw . && stylelint **/*.scss",
    "test": "nyc mocha --compilers js:babel-register --timeout 3500",
    "release": "npm-run-all clean build release:copy release:deps release:package",
    "clean": "rimraf src/public/* dist",
    "lint:watch": "npm run lint -- --watch",
    "test:cover": "nyc --reporter=html npm test",
    "watch:assets": "npm run clean && webpack --watch --colors --display-error-details --config build/webpack.config.js",
    "release:copy": "cross-env-shell \"shx mkdir -p dist/$npm_package_name && shx cp -r src/ build/ *.js *.json .babelrc dist/$npm_package_name\" ",
    "release:deps": "cross-env-shell \"cd dist/$npm_package_name && npm i --production\" ",
    "release:package": "cross-env-shell \"cd dist && tar -zcvf $npm_package_name.tar.gz $npm_package_name\" "
  },
  "pre-commit": [
    "lint"
  ],
  "author": "chenbin92 travisxu",
  "license": "ISC",
  "dependencies": {
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-ejs": "^4.1.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "chalk": "^1.1.3",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "echarts": "^3.7.1",
    "eslint": "^4.1.1",
    "eslint-config-egg": "^4.2.1",
    "eslint-watch": "^3.1.2",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.1",
    "koa-webpack-dev-middleware": "^2.0.0",
    "koa-webpack-middleware": "^1.0.5",
    "mocha": "^3.5.3",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.2.1",
    "open-browser-webpack-plugin": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "pre-commit": "^1.2.2",
    "resolve-path": "^1.3.3",
    "sass-loader": "^6.0.6",
    "should": "^13.1.0",
    "shx": "^0.2.2",
    "style-loader": "^0.18.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.5",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-middleware": "^1.12.0"
  }
}
